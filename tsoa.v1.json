{
    "entryFile": "src/server.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "controllerPathGlobs": ["src/v1/**/*Controller.ts"],
    "spec": {
      "outputDirectory": "./build",
      "specVersion": 3,
      "basePath": "/v1",
      "specFileName": "swagger.v1.json",
      "version": "1.0.0"
    },
    "routes": {
      "routesDir": "./src",
      "middleware": "express",
      "basePath": "/v1",
      "routesFileName": "routes.v1.ts"
    }
}